<h1> Welcome to Makeup App! </h1>

<%= form_for @user do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>
 <%= f.label :email %>
 <%= f.email_field :email %>
 <%= f.label :age %>
 <%= f.number_field :age %>
 <%= f.label :password %>
 <%= f.password_field :password %>
 <%= f.label :password_confirmation %>
 <%= f.password_field :password_confirmation %>
 <%= f.submit "Sign up!" %>
 <% end %>